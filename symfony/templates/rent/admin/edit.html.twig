

{% block body %}
    <h1>Edit Rent {{ rent.id }}</h1>
    <h3>
        {{ rent.movie.title }} -- {{ rent.user.username }}  -- {{ rent.createdAt|date("d/m/Y") }}
    </h3>

    {% if rent.isFinish == false %}
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <input type="submit" value="submit">
        {{ form_end(form) }}
    {% endif %}
{% endblock %}

